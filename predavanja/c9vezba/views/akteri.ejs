<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Akteri</title>
  </head>
  <body>
    <a href="/ruta/homepage">HOMEPAGE</a>
    <a href="/ruta/filmovi">FILMOVI</a>

    <table border="1" width="100%">
      <tr>
        <th>Ime</th>
        <th>Prezime</th>
        <th>Rating</th>
      </tr>
      <% akteri.forEach(s => { %>
      <tr>
        <td><%= s.ime %></td>
        <td><%= s.prezime %></td>
        <td><%= s.rating %></td>
      </tr>
      <% }) %>
    </table>
  </body>
</html>
